{
    "useTabChar": false,
    "tabSize": 4,
    "spaceUnits": 4,
    "closeBrackets": false,
    "showLineNumbers": true,
    "styleActiveLine": false,
    "wordWrap": false,
    "linting.enabled": true,
    "linting.collapsed": true,
    "quickview.enabled": true,
    "language": {
        "javascript": {
            "linting.prefer": "JSHint",
            "linting.usePreferredOnly": true
        }
    },
    "jwolfe.file-tree-exclude.list": [
        "node_modules",
        "lib",
        "dist",
        ".git",
        ".complexity/",
        ".coverage/",
        ".sass-cache/"
    ]
}